<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
	id="WebApp_ID" version="4.0">

	<display-name>Training_Project_Jayapal_Reddy</display-name>
	<servlet>
		<servlet-name>RegistrationProcessControllerServlet</servlet-name>
		<servlet-class>com.tadigital.trainingproject.customer.servlet.RegistrationProcessControllerServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>RegistrationProcessControllerServlet</servlet-name>
		<url-pattern>/register</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>LoginProcessControllerServlet</servlet-name>
		<servlet-class>com.tadigital.trainingproject.customer.servlet.LoginProcessControllerServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>LoginProcessControllerServlet</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>CustomersListProcessControllerServlet</servlet-name>
		<servlet-class>com.tadigital.trainingproject.customer.servlet.CustomersListProcessControllerServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>CustomersListProcessControllerServlet</servlet-name>
		<url-pattern>/customerslist</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>ProductsListProcessControllerServlet</servlet-name>
		<servlet-class>com.tadigital.trainingproject.product.servlet.ProductsListProcessControllerServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ProductsListProcessControllerServlet</servlet-name>
		<url-pattern>/productslist</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>AccountDetailsUpdateProcessControllerServlet</servlet-name>
		<servlet-class>com.tadigital.trainingproject.customer.servlet.AccountDetailsUpdateProcessControllerServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>AccountDetailsUpdateProcessControllerServlet</servlet-name>
		<url-pattern>/updateAccountDetails</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>ChangePasswordProcessControllerServlet</servlet-name>
		<servlet-class>com.tadigital.trainingproject.customer.servlet.ChangePasswordProcessControllerServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ChangePasswordProcessControllerServlet</servlet-name>
		<url-pattern>/changePassword</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
</web-app>